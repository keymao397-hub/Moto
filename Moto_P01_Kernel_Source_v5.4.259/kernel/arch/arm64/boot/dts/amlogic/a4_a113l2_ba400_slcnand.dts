&periphs_pinctrl {

	tdmout_b_pins: tdmout_b_pin {
		mux { /* GPIOT_11, GPIOT_12, GPIOT_13 */
			groups = "tdm_fs2_t11",
				"tdm_sclk2_t12",
				"tdm_d8";
			function = "tdm";
		};
	};

	tdm_b_pins: tdm_b_pin {
		mux {
			/*
			 * GPIOT_2, GPIOT_3, GPIOT_0, GPIOT_1, GPIOT_5, GPIOT_6,
			 * GPIOT_7, GPIOT_8, GPIOT_18, GPIOT_19
			 */
			groups = "tdm_fs1_t2",
				"tdm_sclk1_t3",
				"tdm_d2_t0",
				"tdm_d3_t1",
				"tdm_d4_t5",
				"tdm_d5_t6",
				"tdm_d6_t7",
				"tdm_d7",
				"tdm_d12_t18",
				"tdm_d13_t19";
			function = "tdm";
		};
	};

	tdmout_b_gpios: tdmout_b_gpios {
		mux {
			groups = "GPIOT_11",
				"GPIOT_12",
				"GPIOT_13";
			function = "gpio_periphs";
			output-low;
		};
	};

	tdmin_c_gpios: tdmin_c_gpios {
		mux {
			groups = "GPIOT_14";
			function = "gpio_periphs";
			output-low;
		};
	};

	tdmin_c_pins: tdmin_c_pin {
		mux { /* GPIOT_14 */
			groups = "tdm_d9";
			function = "tdm";
		};
	};
	tdmout_b_gpio: tdmout_b_gpio {
		mux {
			groups ="GPIOT_0",
				"GPIOT_1",
				"GPIOT_2",
				"GPIOT_3",
				"GPIOT_4",
				"GPIOT_5",
				"GPIOT_6",
				"GPIOT_7",
				"GPIOT_8",
				"GPIOT_18",
				"GPIOT_19";
			function = "gpio_periphs";
			output-low;
		};
	};
	tdmin_c_gpio: tdmin_c_gpio {
		mux {
			groups ="GPIOX_7",
				"GPIOX_8",
				"GPIOX_9",
				"GPIOX_10";
			function = "gpio_periphs";
			output-low;
		};
	};
	tdm_a_pins: tdm_a_pin {
		mux { /* GPIOX_7, GPIOX_8, GPIOX_9, GPIOX_10, */
			groups = "tdm_d1",
				"tdm_d0_x8",
				"tdm_fs0_x9",
				"tdm_sclk0_x10";
			function = "tdm";
		};
	};

	pdmin: pdmin {
		mux { /* GPIOD_2, GPIOD_3, GPIOD_4 */
			groups = "pdm_din1_d2",
				"pdm_din0_d3",
				"pdm_dclk_d4";
			function = "pdm";
		};
	};

}; /* end of periphs_pinctrl */

